                //requeridos
                var calle = $(detalles[i]).find("input#DireccionEntrega_Calle").val();
                if (calle != null && calle != "") {
                    //$($('.DireccionEntrega')[0]).find("input#Calle").val();
                    //$($('.DireccionEntrega #Calle')[0]).val();
                    var item = {
                        Calle: calle,
                        Id: $(detalles[i]).find("input#DireccionEntrega_Id").val(),
                        Numero: $(detalles[i]).find("input#DireccionEntrega_Numero").val(),
                        Piso: $(detalles[i]).find("input#DireccionEntrega_Piso").val(),
                        Departamento: $(detalles[i]).find("input#DireccionEntrega_Departamento").val(),
                        CodigoPostal: $(detalles[i]).find("input#DireccionEntrega_CodigoPostal").val(),
                        Telefono: $(detalles[i]).find("input#DireccionEntrega_Telefono").val(),
                        Email: $(detalles[i]).find("input#DireccionEntrega_Email").val(),
                    };
                    Cliente.DireccionesEntrega.push(item);
                }
            }

            var postParams = {
                url: "@Url.Action("Editar")",
                data: { Cliente: Cliente },
                success: function (data) {
                    if (data.Success) {
                        msg = "La operación fue existosa";
                        typeMsg = "success";
                        ClienteEditar.OnFinishSave(typeMsg, msg);
                    }
                    else {
                        $('#BtnSubmit').removeAttr('disabled');
                        $('#Alert-Container').NewAlert('error', data.ErrorMessage);
                    }
                }
            }
            $.AjaxPost(postParams);
        },

            OnFinishSave: function (typeMsg, msg) {
                $.WarningDialog(msg, typeMsg, function () {
                    window.onbeforeunload = null;
                    $.ShowLoader();
                    window.location = '@Url.Action("Index")';
                });
            }

